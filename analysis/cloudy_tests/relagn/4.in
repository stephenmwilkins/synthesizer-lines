table SED "1.sed" 
abundances "solar_GASS10.abn"
metals 0.1
ionization parameter 0
cosmic rays, background
hden 7.0 log constant density
set temperature floor 100 linear
stop temperature 500K
stop efrac -2
set continuum resolution 1.0
save overview  "4.ovr" last
save last abundances "4.abundances"
save last continuum "4.cont" units Angstroms no clobber
save line list column absolute last units angstroms                   "4.elin" "linelist.dat"
save last lines, array "4.lines" units Angstroms no clobber